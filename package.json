{
	"name": "ReactION",
	"displayName": "ReactION",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ReactION.openTree",
		"onCommand:ReactION.openWeb",
		"onWebviewPanel:ReactION",
		"onView:startExtension"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"ReactION-button": [
				{
					"id": "startExtension",
					"name": "Targets"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ReactION-button",
					"title": "ReactION-button",
					"icon": "resources/dep.svg"
				}
			]
		},
		"commands": [
			{
				"command": "ReactION.openTree",
				"title": "ReactION Tree"
			},
			{
				"command": "ReactION.openWeb",
				"title": "ReactION Web"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"react-dev": "react-devtools"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"chrome-launcher": "^0.10.5",
		"electron-devtools-installer": "^2.2.4",
		"puppeteer": "^1.12.2",
		"react-devtools": "^3.6.1"
	}
}
