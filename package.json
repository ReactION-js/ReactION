{
	"name": "projectX",
	"displayName": "projectX",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:projectX.openTree",
		"onCommand:projectX.openWeb",
		"onWebviewPanel:projectX"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "projectX.openTree",
				"title": "px"
			},
			{
				"command": "projectX.openWeb",
				"title": "pxtest"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"chrome-launcher": "^0.10.5",
		"puppeteer": "^1.12.2",
		"puppeteer-core": "^1.12.2"
	}
}
